{
  "name": "of-task",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:all": "jest --selectProjects unit integration",
    "test:all:watch": "jest --selectProjects unit integration --watch",
    "test:all:coverage": "jest --selectProjects unit integration --coverage",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:unit:watch": "jest --selectProjects unit --watch",
    "test:unit:coverage": "jest --selectProjects unit --coverage",
    "build": "npm run build:clean && tsc",
    "build:clean": "tsc --build --clean && rm -rf dist",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "npm run build && node dist/index.js"
  },
  "keywords": [],
